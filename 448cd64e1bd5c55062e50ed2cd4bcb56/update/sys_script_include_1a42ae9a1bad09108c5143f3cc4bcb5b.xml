        <access>public</access>
        <active>true</active>
        <api_name>global.CatItemsUserCanAccess</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description/>
        <name>CatItemsUserCanAccess</name>
        <script><![CDATA[function CatItemsUserCanAccess(userId) { 

	var catItemArr  = [];
	if(!gs.nil(userId)){
		var catItems = new GlideRecord('sc_cat_item');
		catItems.addActiveQuery();
		catItems.query();
		while(catItems.next()){
			var ucu = new UserCriteriaUtil('', userId);
			ucu.cat_item = catItems.sys_id.toString();
			if(ucu.catItemCheck()){
				catItemArr.push(ucu.cat_item);
			}
		}
	}
	return catItemArr;
}]]></script>
